{
    "name": "amx/package-boilerplate",
    "type": "library",
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Amx\\PackageBoilerplate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amx\\PackageBoilerplate\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/.vscode",
            "/.idea",
            "/tests",
            "/.gitattributes",
            "composer.json",
            "composer.lock",
            "README.md",
            "eslint.config.js",
            ".prettierignore",
            ".prettierrc"
        ]
    }
}
